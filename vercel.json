{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "docusaurus",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat.js"
    },
    {
      "source": "/api/chat/",
      "destination": "/api/chat.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1",
      "continue": true
    }
  ]
}
